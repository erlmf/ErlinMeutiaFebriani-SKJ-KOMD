	#include<iostream>
	using namespace std;
	
	int main(){
	    // declare the variables
	    int num1 = 5;
	    int num2 = 10;
	    int result = 0;
	
	    // operating process
	    result = num1 * num2;
	    // print the multiplication result
	    cout << "The result = " << result << endl;
	
	    return 0;
	
	}
